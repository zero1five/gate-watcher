{
  "name": "gate-watcher",
  "version": "0.0.1",
  "description": "üçì check your javascript object is charming",
  "main": "lib/gate-watcher.cjs.js",
  "files": [
    "lib"
  ],
  "keywords": [],
  "scripts": {
    "build": "bili -o lib",
    "lint": "eslint src --fix",
    "test": "jest --coverage",
    "commit": "git-cz",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0",
    "release": "release-it --no-git.requireCleanWorkingDir",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "url": "https://github.com/zero1five/gate-watcher",
    "type": "git"
  },
  "author": "zero1five<zerodengyin@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@commitlint/read": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "bili": "^3.4.2",
    "chalk": "^2.4.1",
    "commitizen": "^3.0.5",
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-coffee-preprocessor": "^1.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "release-it": "^9.4.3"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{js}": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "node ./scripts/commit-lint.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
